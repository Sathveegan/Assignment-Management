<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>AssignmentManagement</display-name>
  <welcome-file-list>
    <welcome-file>lhome</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentAssignServlet</servlet-name>
    <servlet-class>servlet.StudentAssignServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentAssignServlet</servlet-name>
    <url-pattern>/shome</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LecturerAssignServlet</servlet-name>
    <servlet-class>servlet.LecturerAssignServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LecturerAssignServlet</servlet-name>
    <url-pattern>/lhome</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DownloadFileServlet</servlet-name>
    <servlet-class>servlet.DownloadFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownloadFileServlet</servlet-name>
    <url-pattern>/downloadFile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MySubmissionServlet</servlet-name>
    <servlet-class>servlet.MySubmissionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MySubmissionServlet</servlet-name>
    <url-pattern>/my-submission</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentSubmissionServlet</servlet-name>
    <servlet-class>servlet.StudentSubmissionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentSubmissionServlet</servlet-name>
    <url-pattern>/student-submission</url-pattern>
  </servlet-mapping>
</web-app>